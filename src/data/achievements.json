    "condition": ["(() => {",
      "for(let key in data.elements){",
      " let exotic = data.elements[key].exotic;",
      "  if (player.resources[exotic].unlocked) {",
      "    return true;",
      "  }",
      "}",
      "return false;",
      "})()"
    ]
  }
}
